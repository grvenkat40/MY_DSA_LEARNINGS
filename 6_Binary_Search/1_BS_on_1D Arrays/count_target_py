class Solution:
    def countOccurrences(self, nums, target):
        i=0
        j=len(nums)-1
        cnt=0
        while i<=j:
            if nums[i]<target:
                i+=1
            elif nums[j]>target:
                j-=1
            else:
                cnt+=1
                i+=1
                # j-=1
        return cnt

# nums = [0, 0, 1, 1, 1, 2, 3]
# target = 1
nums = [5, 5, 5, 5, 5, 5]
target = 5
obj=Solution()
print(obj.countOccurrences(nums,target))